{
    "manifest_version": 3,
    "name": "Put This Script in Website",
    "version": "25.2",
    "description": "allows you to embed your javascript code locally on the site",
    "sandbox": {
      "pages": ["sandbox.html"]
    },
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "res/ptsiw.png",
        "48": "res/ptsiw.png",
        "128": "res/ptsiw.png"
      }
    },
    "icons": {
      "16": "res/ptsiw.png",
      "48": "res/ptsiw.png",
      "128": "res/ptsiw.png"
    }
  }